{
  "name": "@counterfactual/wallet-ui",
  "version": "0.1.2",
  "private": true,
  "dependencies": {
    "eth-balance-checker": "0.1.1",
    "ethers": "4.0.37",
    "node-sass": "4.12.0",
    "react": "16.10.1",
    "react-dom": "16.10.1",
    "react-redux": "7.1.1",
    "react-router-dom": "5.1.1",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:e2e": "REACT_APP_HUB_API_URL=http://localhost:9001 CI=true react-scripts start",
    "build": "react-scripts build",
    "test": "CI=true react-scripts test",
    "test:coverage": "CI=true react-scripts test --coverage",
    "eject": "react-scripts eject",
    "lint": "tslint -c tslint.json -p .",
    "lint:fix": "tslint -c tslint.json -p . --fix"
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**/*.ts*",
      "!./src/index.tsx",
      "!./src/**/*.mock.ts",
      "!./src/**/*.d.ts",
      "!./src/**/types.ts",
      "!./src/**/index.ts",
      "!./src/utils/log.ts",
      "!./src/utils/testSelector.ts",
      "!./src/store/test-utils/*.ts",
      "!./src/store/store.ts"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "3.10.3",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/jest": "24.0.15",
    "@types/node": "12.7.8",
    "@types/react": "16.9.3",
    "@types/react-dom": "16.9.1",
    "@types/react-redux": "7.1.4",
    "@types/react-router-dom": "5.1.0",
    "@types/web3": "1.0.20",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "jest-fetch-mock": "2.1.2",
    "react-scripts": "3.0.1",
    "typescript": "3.5.3"
  }
}
