{
  "name": "@counterfactual/machine",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/src/index.d.ts",
  "module": "dist/index.es.js",
  "iife": "dist/index-iife.js",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc -p . && rollup -c",
    "test": "yarn ganache:start && jest --detectOpenHandles && yarn ganache:stop",
    "ganache:start": "ganache-cli -q -i 8888 -l 0xfffffffffff -g 0x01 > /dev/null &",
    "ganache:stop": "ps aux | grep ganache-cli | grep -v grep | awk '{print $2}' | xargs kill -9",
    "lint:fix": "yarn lint --fix",
    "lint": "tslint -c tslint.json -p ."
  },
  "devDependencies": {
    "@counterfactual/types": "0.0.1",
    "@counterfactual/typescript-typings": "0.0.1",
    "@types/chai": "^4.1.7",
    "@types/jest": "^23.3.3",
    "@types/node": "^10.9.3",
    "chai": "^4.2.0",
    "ganache-cli": "6.2.4",
    "jest": "^23.6.0",
    "rollup": "^0.68.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-typescript2": "^0.18.0",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "typescript": "^3.1.2",
    "typescript-memoize": "^1.0.0-alpha.3"
  },
  "dependencies": {
    "@counterfactual/contracts": "0.0.3",
    "ethers": "4.0.20"
  }
}
