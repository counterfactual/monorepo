  },
  "homepage": "https://counterfactual.com",
  "devDependencies": {
    "@stencil/sass": "^0.1.1",
    "@types/jest": "23.3.14",
    "@types/puppeteer": "1.12.3",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "puppeteer": "1.14.0",
    "typescript": "^3.3.4000",
    "workbox-build": "3.4.1"
  }
